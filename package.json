{
  "name": "dynamic-ui",
  "version": "0.0.1",
  "description": "This project was created for fast generate user interface from JSON data",
  "repository": "https://github.com/AnthonySchurov/dynamic_ui",
  "bugs": "https://github.com/AnthonySchurov/dynamic_ui/issues",
  "license": "MIT",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js  --recursive",
    "test:watch": "npm run test -- --watch",
    "run": "node tools/run.js",
    "build": "node tools/build.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.19",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.1.18",
    "babel-plugin-transform-react-constant-elements": "^6.1.20",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.2",
    "commander": "^2.9.0",
    "fs": "0.0.2",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "path": "^0.12.7",
    "promise": "^7.0.4",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "history": "^1.13.1",
    "immutable": "^3.7.5",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-router": "^1.0.0"
  }
}
